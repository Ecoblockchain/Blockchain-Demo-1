@model BlockChainWebApplication.Models.Request

@{
    ViewBag.Title = "Asset Requests";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section JavaScript{
    <script type="text/javascript">$(document).ready(function () { WebApp.Core.HighlightCurrentMenuItem('My Wallet') });</script>
}

@section CSS{

}

<div class="page-header">
    <div class="page-header-content">
        <div class="page-title">
            <h4><span class="text-semibold">Asset Requests</span></h4>
        </div>
        <div class="heading-elements">
            <div class="heading-btn-group">
                <a class="btn btn-link btn-float has-text" href="/wallet/request"><i class="icon-googleplus5 text-primary"></i><span>Request Asset</span></a>

            </div>
        </div>
    </div>

</div>
<div class="content">
    <div class="panel panel-flat">
        <table class="table data-table table-bordered table-striped table-hover" data-source="/wallet/list/" data-key="4">
            <thead>
                <tr>
                    <th>
                        User
                    </th>
                    <th>
                        Assets
                    </th>

                    <th>
                        Quantity
                    </th>

                    <th>
                        Status
                    </th>
                    <th>
                        Created On
                    </th>
                    <th>
                        Status Updated On
                    </th>
                    <th>Options</th>


                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

</div>
